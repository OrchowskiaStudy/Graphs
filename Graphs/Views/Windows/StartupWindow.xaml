<Window x:Class="Graphs.StartupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graphs"
        mc:Ignorable="d"
        xmlns:graphs="clr-namespace:GraphSharp.Controls;assembly=GraphSharp.Controls"
        MinHeight="500"
        MinWidth="500"
        Title="MainWindow">
    <Window.Resources>

    </Window.Resources>
    <Grid Background="{StaticResource MenuBackGround}">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" MinWidth="70"/>
                <ColumnDefinition Width="5*"/>
            </Grid.ColumnDefinitions>
            <Button Margin="10" Content="Go to menu" Foreground="Black" Click="Button_Click"/>            
        </Grid>
        <Grid Grid.Row="1">
            <DockPanel x:Name="dock">
                <Frame x:Name="_content" NavigationUIVisibility="Hidden" />
            </DockPanel>
        </Grid>
    </Grid>
</Window>
