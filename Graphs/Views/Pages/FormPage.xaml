<local:PageBase x:Class="Graphs.Views.Pages.FormPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Graphs.Views.Pages"
      xmlns:vm="clr-namespace:Graphs.ViewModels"
      xmlns:controls="clr-namespace:Graphs.Views.Controls"
      mc:Ignorable="d" 
      Title="AdiacencyMatrix">
    <Page.Resources>
        <Style TargetType="{x:Type TextBlock}">
            <Setter Property="Foreground" Value="White"/>
        </Style>
    </Page.Resources>
    <Grid>
        <controls:AdiacencyMatrixInput Width="500" Height="500"/>
    </Grid>
</local:PageBase>
